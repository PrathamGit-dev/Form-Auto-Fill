{
    "name": "Form Auto Fill",
    "description": "A chrome extension to automatically fill up most common unchanging fields in a Google Form.",
    "version":"1.0",
    "manifest_version":3,
    "background":{
        "service_worker":"background.js"
    },
    "permissions": ["storage","activeTab","scripting"],
    "action":{
        "default_popup":"popup.html",
        "default_icon": "/icon128.jpg"
    },
    "icons": {
        "16":"/icon16.jpg",
        "32": "/icon32.jpg",
        "48": "/icon48.jpg",
        "128": "/icon128.jpg"
    }
}